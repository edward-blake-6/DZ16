{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "description": "ID пользователя"
    },
    "username": {
      "type": "string",
      "description": "Имя пользователя"
    },
    "firstName": {
      "type": "string",
      "description": "Имя"
    },
    "lastName": {
      "type": "string",
      "description": "Фамилия"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Email"
    },
    "password": {
      "type": "string",
      "description": "Пароль"
    },
    "phone": {
      "type": "string",
      "description": "Телефон"
    },
    "userStatus": {
      "type": "integer",
      "description": "Статус пользователя"
    }
  },
  "required": ["id", "username", "firstName", "lastName", "email", "password", "phone", "userStatus"],
  "additionalProperties": false
}